<window width="unsigned int" height="unsigned int" title="str">
    <image x="float"
           y="float"
           width="optional float"
           height="optional float"
           src="str"/>
    <text x="float"
          y="float"
          font="str"
          size="unsigned int"
          outline="float"
          color="str"
          outlineColor="str"
          bold="bool"
          italic="bool"
          underline="bool"
          strike="bool">
        {content:str}
    </text>
    <rectangle x="float"
               y="float"
               width="float"
               height="float"
               outline="float"
               color="str"
               outlineColor="str"/>
    <frame x="float"
           y="float"
           width="unsigned int"
           height="unsigned int">
        {patterns:[image|rectangle|text|frame]}
    </frame>
</window>

