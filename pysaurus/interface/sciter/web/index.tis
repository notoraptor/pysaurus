event ready {
	stdout.println(Integer.MIN, Integer.MAX);
    const (sx, sy, sw, sh) = view.screenBox(#workarea, #rectw);
    const width = 1200;
    const height = 720;
    const x = (sw - width) / 2;
    const y = (sh - height) / 2;
    view.move(x, y, width, height);
}
event closerequest {
    view.close_app();
}
